{
    "manifest_version": 3,
    "name": "AimHighTab",
    "description":"Transform your New Tab Page into an authentic personal dashboard",
    "version": "1.0.0",
    "background": {
        "service_worker": "background.js"
    },
    "chrome_url_overrides": {
        "newtab":"newtab.html"
    },
    "permissions": [
        "activeTab",
        "contextMenus",
        "storage",
        "tabs",
        "file://*/*" 
      ],
      "icons": {
        "16": "/logo16.png",
        "48": "/logo48.png",
        "128": "/logo128.png"
    }
}  